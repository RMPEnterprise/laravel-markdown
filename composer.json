{
    "name": "RMPEnterprise/laravel-markdown",
    "description": "Markdown compiler for Laravel and Lumen 5",
    "keywords": ["laravel", "lumen", "md", "markdown", "illuminate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Nisbet",
            "email": "ian.nisbet@rmpenterprise.co.uk"
        }
    ],
    "require": {
        "michelf/php-markdown": "~1.8"
    },
    "require-dev": {
        "mockery/mockery": "0.9.0"
    },
    "autoload": {
        "psr-0": {"RMPEnterprise\\LaravelMarkdown": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "RMPEnterprise\\LaravelMarkdown\\Providers\\LaravelMarkdownServiceProvider"
            ],
            "aliases": {
                "Markdown": "RMPEnterprise\\LaravelMarkdown\\Facades\\LaravelMarkdown"
            }
        }
    },
    "minimum-stability": "dev"
}
